{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "no-console": "off",
    "semi": ["error", "never"],
    "prettier/prettier": "error",
  },
  "settings": {
    "react": {
    "version": "latest"
    }
  }
}